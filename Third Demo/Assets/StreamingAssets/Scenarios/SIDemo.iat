			"AgentDialogues": [
				{
					"Id": "70c58954-f0a7-4c14-b5b1-aaffb59ae785",
					"CurrentState": "Answer_no",
					"NextState": "Last",
					"Meaning": "Answer",
					"Style": "GoodMood",
					"Utterance": "No problem, it can be a bit overwhelming when you first arrive!"
				}, 
				{
					"Id": "0b6c3f29-2082-4e26-8482-c9d5cd59b649",
					"CurrentState": "Answer_yes",
					"NextState": "Last",
					"Meaning": "Answer",
					"Style": "GoodMood",
					"Utterance": "That’s great because they can’t wait to meet you!"
				}, 
				{
					"Id": "aa9b7982-bb79-4c7e-b378-6ffd8aeabb1c",
					"CurrentState": "Greeting",
					"NextState": "Re_Greeting",
					"Meaning": "Greeting",
					"Style": "GoodMood",
					"Utterance": "Hello, I'm Sarah Newton. Welcome to Omosa! Sorry I couldn't welcome you in person. I bet you're wondering why you are there."
				}, 
				{
					"Id": "5c2678ff-5116-41fb-9c07-bb51f112f314",
					"CurrentState": "Intro_continue1",
					"NextState": "Intro_continue2",
					"Meaning": "Introduction",
					"Style": "GoodMood",
					"Utterance": "The Indigenous people on Omosa have started to report that the animals they hunt have started to die out and a few other worrying changes to the ecosystem. It's our job to sort this stuff out and help where we can."
				}, 
				{
					"Id": "bbbb7f0e-ff46-4b19-a06d-efa2572d2bea",
					"CurrentState": "Intro_continue2",
					"NextState": "Personality1",
					"Meaning": "ntroduction",
					"Style": "GoodMood",
					"Utterance": "Your job is to collect the data from the other researchers who are working on the island as well as to talk to some of the Omosans living in the local village."
				}, 
				{
					"Id": "99d202c0-1d8b-4557-9d1a-f74818ed5da1",
					"CurrentState": "Intro_no",
					"NextState": "Intro_continue1",
					"Meaning": "Introduction",
					"Style": "GoodMood",
					"Utterance": "Unfortunately not. You're here to help us to uncover the source of an ecological crisis!"
				}, 
				{
					"Id": "78c326e8-a423-4b6b-880b-519aeb542c87",
					"CurrentState": "Intro_yes",
					"NextState": "Intro_continue1",
					"Meaning": "Introduction",
					"Style": "GoodMood",
					"Utterance": "You're here to help us to uncover the source of an ecological crisis!"
				}, 
				{
					"Id": "d959a668-fffa-4b5e-9990-341c785ab35c",
					"CurrentState": "Last",
					"NextState": "End",
					"Meaning": "End",
					"Style": "GoodMood",
					"Utterance": "After you’ve gathered as much data as you can, I want you to prepare a presentation on your theory of what is causing these ecological changes. Remember to take notes as you talk to people, and good luck!"
				}, 
				{
					"Id": "57958bd2-2390-4e59-9272-d0e3f224e8c5",
					"CurrentState": "Personality1",
					"NextState": "Personality2",
					"Meaning": "PersonalityTest",
					"Style": "GoodMood",
					"Utterance": "You are going to meet Charlie the ecologist in the research lab. He gets upset easily but he’s always willing to try something new at work."
				}, 
				{
					"Id": "de6584ce-8b6b-480a-ae40-8cd5fbe4a6e2",
					"CurrentState": "Personality10",
					"NextState": "Re_Personality10",
					"Meaning": "PersonalityTest",
					"Style": "GoodMood",
					"Utterance": "Do you tend to be warm and caring or critical and quarrelsome?"
				}, 
				{
					"Id": "5e4d7720-6610-464f-8d02-072e6ee6f9e0",
					"CurrentState": "Personality2",
					"NextState": "Re_Personality2",
					"Meaning": "PersonalityTest",
					"Style": "GoodMood",
					"Utterance": "Do you get easily upset or keep calm?"
				}, 
				{
					"Id": "554865a3-d3fe-4abd-9828-a0b8ad14c97c",
					"CurrentState": "Personality3",
					"NextState": "Personality4",
					"Meaning": "PersonalityTest",
					"Style": "GoodMood",
					"Utterance": "You will also meet Zafirah the climatologist in the weather station. She is very creative and can be critical about other people’s ideas. "
				}, 
				{
					"Id": "6213efcd-b7cb-477e-af60-6b73f123fe24",
					"CurrentState": "Personality4",
					"NextState": "Re_Personality4",
					"Meaning": "PersonalityTest",
					"Style": "GoodMood",
					"Utterance": "Would you say you are open to new ideas and experiences or prefer things to stay the same?"
				}, 
				{
					"Id": "26dd8c16-a6fd-4c4b-a91b-848183bc36aa",
					"CurrentState": "Personality5",
					"NextState": "Personality6",
					"Meaning": "PersonalityTest",
					"Style": "GoodMood",
					"Utterance": "In the village you will meet Lyina the hunter. She is enthusiastic and somewhat disorganized. "
				}, 
				{
					"Id": "0e22ac87-62d6-48e4-b460-9bc092eff6d5",
					"CurrentState": "Personality6",
					"NextState": "Re_Personality6",
					"Meaning": "PersonalityTest",
					"Style": "GoodMood",
					"Utterance": "Do you tend to be dependable and self-disciplined or disorganized and careless?"
